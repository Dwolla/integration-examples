        "core-js": "^3.24.1",
        "dwolla-v2": "4.0.0-ts-alpha.0",
        "form-data": "^4.0.0",
        "next": "12.2.4",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-script-hook": "^1.7.0"
    }
